{
	"private": true,
	"name": "auth-server",
	"version": "0.0.0-dev.0",
	"description": "example standalone auth server",
	"main": "index.js",
	"scripts": {
		"prepare": "run-s clean && run-p build-server build-clientside && run-s test",
		"clean": "rm -rf dist && mkdir dist",
		"build-server": "tsc",
		"build-clientside": "tsc -p source/clientside && browserify dist/clientside/login-script.js > dist/clientside/login-script.bundle.js",
		"watch": "tsc -w",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"keywords": [],
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian-client.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian-client/issues"
	},
	"homepage": "https://github.com/chase-moskal/authoritarian-client#readme",
	"dependencies": {
		"@types/koa-route": "3.2.4",
		"authoritarian": "file:../authoritarian",
		"express": "4.16.4",
		"google-auth-library": "3.0.1",
		"koa": "2.6.2",
		"koa-bodyparser": "4.2.1",
		"koa-mount": "4.0.0",
		"koa-route": "3.2.0",
		"koa-router": "7.4.0",
		"koa-static": "5.0.0",
		"koa-static-server": "1.4.0",
		"passport": "0.4.0",
		"passport-google-oauth": "1.0.0",
		"pug": "2.0.3"
	},
	"devDependencies": {
		"@types/express": "4.16.0",
		"@types/koa": "2.0.47",
		"@types/koa-bodyparser": "4.2.1",
		"@types/koa-mount": "3.0.1",
		"@types/koa-router": "7.0.35",
		"@types/koa-static": "4.0.0",
		"@types/koa-static-server": "1.3.0",
		"@types/node": "10.12.12",
		"@types/passport": "0.4.7",
		"@types/passport-google-oauth": "1.0.39",
		"@types/pug": "2.0.4",
		"browserify": "16.2.3",
		"npm-run-all": "4.1.5",
		"typescript": "3.2.2"
	}
}
