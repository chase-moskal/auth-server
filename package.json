{
	"private": true,
	"name": "auth-server",
	"version": "0.0.0-dev.2",
	"description": "example standalone auth server",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"sourceType": "module",
	"type": "module",
	"main": "dist/main.js",
	"scripts": {
		"prepare": "run-s clean && run-p compile importmap && run-s test",
		"clean": "rm -rf dist && mkdir -p dist/clientside",
		"compile": "tsc",
		"importmap": "importly --host=node_modules < package.json > dist/clientside/importmap.json",
		"watch": "tsc -w",
		"generate-keys": "./node_modules/authoritarian/generate-keys && mv private.pem config/auth-server.private.pem && mv public.pem config/auth-server.public.pem",
		"start": "node dist/main",
		"test": "exit 0"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian-client.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian-client/issues"
	},
	"homepage": "https://github.com/chase-moskal/authoritarian-client#readme",
	"dependencies": {
		"@koa/cors": "^3.0.0",
		"authoritarian": "0.0.0-dev.35",
		"crosscall": "0.0.6-dev.16",
		"express": "^4.17.1",
		"google-auth-library": "^5.7.0",
		"importly": "0.1.0-dev.0",
		"koa": "^2.11.0",
		"koa-bodyparser": "^4.2.1",
		"koa-mount": "^4.0.0",
		"koa-static": "^5.0.0",
		"mongodb": "^3.4.1",
		"passport": "^0.4.1",
		"passport-google-oauth": "^2.0.0",
		"pug": "^2.0.4",
		"redcrypto": "0.0.0-dev.3",
		"renraku": "0.0.0-dev.19",
		"sinon": "^8.0.2",
		"unique-names-generator": "^4.1.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/gapi.auth2": "0.0.51",
		"@types/jest": "^24.0.25",
		"@types/koa": "^2.11.0",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-compose": "^3.2.5",
		"@types/koa-mount": "^4.0.0",
		"@types/koa-static": "^4.0.1",
		"@types/koa__cors": "^3.0.0",
		"@types/mongodb": "^3.3.14",
		"@types/node": "^13.1.4",
		"@types/passport": "^1.0.2",
		"@types/passport-google-oauth": "^1.0.41",
		"@types/pug": "^2.0.4",
		"browserify": "^16.5.0",
		"jest": "^24.9.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^3.7.4"
	}
}
